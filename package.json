{
  "name": "vrcfriendcheck",
  "version": "1.2.1",
  "description": "VRChatにてフレンドのオンライ状況が確認できる拡張機能です",
  "author": "mnao305 <naomasa305@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
    "releaseCWS": "cd dist && webstore upload --auto-publish",
    "releaseAMO": "cd dist && web-ext-submit",
    "release": "npm run releaseCWS"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/vue-fontawesome": "0.1.6",
    "axios": "0.19.0",
    "vue": "2.6.10",
    "vue-router": "3.0.6",
    "webextension-polyfill": "0.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "archiver": "3.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "chrome-webstore-upload-cli": "1.2.0",
    "copy-webpack-plugin": "5.0.3",
    "core-js": "3.1.3",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "ejs": "2.6.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "file-loader": "3.0.1",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.12.0",
    "sass-loader": "7.1.0",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "2.6.10",
    "web-ext-submit": "3.0.0",
    "web-ext-types": "3.1.0",
    "webpack": "4.32.2",
    "webpack-chrome-extension-reloader": "1.3.0",
    "webpack-cli": "3.3.2",
    "webpack-shell-plugin": "0.5.0"
  }
}
