{
  "name": "vrcfriendcheck",
  "version": "1.3.2",
  "description": "VRChatにてフレンドのオンライ状況が確認できる拡張機能です",
  "author": "mnao305 <naomasa305@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
    "releaseCWS": "cd dist && webstore upload --auto-publish",
    "releaseAMO": "cd dist && web-ext-submit",
    "release": "npm run releaseCWS"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "axios": "0.19.2",
    "vue": "2.6.11",
    "vue-router": "3.1.6",
    "webextension-polyfill": "0.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/preset-env": "7.9.6",
    "archiver": "4.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "chrome-webstore-upload-cli": "1.2.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.5",
    "cross-env": "6.0.3",
    "css-loader": "3.5.3",
    "ejs": "3.1.2",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "3.0.4",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.2.2",
    "file-loader": "5.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "sass-loader": "8.0.2",
    "vue-loader": "15.9.2",
    "vue-template-compiler": "2.6.11",
    "web-ext-submit": "3.2.1",
    "web-ext-types": "3.2.1",
    "webpack": "4.43.0",
    "webpack-chrome-extension-reloader": "1.3.0",
    "webpack-cli": "3.3.11",
    "webpack-shell-plugin": "0.5.0"
  }
}
