{
  "name": "vrcfriendcheck",
  "version": "1.5.6",
  "description": "VRChatにてフレンドのオンライ状況が確認できる拡張機能です",
  "author": "mnao305 <naomasa305@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue,.ts src",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
    "releaseCWS": "cd dist && webstore upload --auto-publish",
    "releaseAMO": "cd dist && web-ext-submit",
    "release": "npm run releaseCWS"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/vue-fontawesome": "0.1.10",
    "vrcapi-client": "0.4.0",
    "vue": "2.6.12",
    "vue-router": "3.5.1",
    "webextension-polyfill": "0.8.0",
    "webextension-polyfill-ts": "0.25.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-optional-chaining": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "archiver": "3.1.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "chrome-webstore-upload-cli": "1.2.1",
    "copy-webpack-plugin": "5.1.2",
    "core-js": "3.12.1",
    "cross-env": "7.0.3",
    "css-loader": "3.6.0",
    "ejs": "3.1.6",
    "eslint": "7.25.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "7.9.0",
    "file-loader": "6.2.0",
    "mini-css-extract-plugin": "0.12.0",
    "node-sass": "4.14.1",
    "sass-loader": "8.0.2",
    "ts-loader": "8.2.0",
    "typescript": "4.2.4",
    "vue-loader": "15.9.7",
    "vue-template-compiler": "2.6.12",
    "web-ext-submit": "3.2.1",
    "web-ext-types": "3.2.1",
    "webpack": "4.46.0",
